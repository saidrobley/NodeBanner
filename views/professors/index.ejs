<% include ../partials/header %>

<h1>Professors</h1>

<table class="pretty-table">
    <thead>
    <tr >
        <th scope="col">Name</th>
        <th colspan="1"></th>
        <th colspan="1"></th>
    </tr>
    </thead>

    <tbody>
    <%  professors.forEach(function (prof) { %>
    <tr>
        <td><%= prof.name %></td>
        <td><button> <a href="/professors/show/<%= prof._id%>"> Show</a></button></td>
        <td><button> <a href="/professors/delete/<%= prof._id%>"> Delete</a></button></td>

    </tr>
    <% }); %>
    </tbody>
</table>
<br><br>

<button><a href="/professors/new">Add New Professor</a></button>


<% include ../partials/footer %>